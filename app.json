{
  "expo": {
    "name": "DataGuardian",
    "slug": "dataguardian",
    "scheme": "dataguardian",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.INTERNET"
      ]
    },
    "plugins": [
      "expo-router"
    ],
    "newArchEnabled": true
  }
}
